{
    "name": "unlv_wads/special",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "woocommerce": {
            "type": "package",
            "package": {
                "name": "woocommerce/flexslider",
                "version": "2.7.2",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/woocommerce/FlexSlider.git",
                    "type": "git",
                    "reference": "2.7.2"
                }
            }
        },
        "newspapers": {
            "type": "vcs",
            "url": "https://bitbucket.org/unlv_wads/newspapers.git"
        },
        "views_nested_details": {
            "type": "package",
	    "package": {
                "name": "natkeeran/views_nested_details",
		"version": "0.0.0",
                "type": "drupal-module",
	        "source": {
                    "url": "https://github.com/Natkeeran/views_nested_details",
                    "type": "git",
		    "reference": "main"
	        }
	    }
        },
        "islandora_local": {
            "type": "vcs",
            "url": "https://bitbucket.org/unlv_wads/islandora_local.git"
        },
	"special_theme": {
	    "type": "package",
	    "package": {
              "name": "unlv_wads/special_theme",
	      "version": "1.0.0",
	      "type": "drupal-theme",
	      "source": {
                "type": "git",
		"reference": "master",
	        "url": "https://bitbucket.org/unlv_wads/special_theme.git"
	      }
	    }
	}
    },
    "require": {
        "php": "^5.5.9|>=7.0.8",
        "composer/installers": "^2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^2.0",
        "drupal/archivesspace": "^1",
        "drupal/authorization": "*",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/blazy": "^2.1",
        "drupal/config_ignore": "^2.2",
        "drupal/config_split": "^1.6",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/devel": "^4",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/entity_browser": "^2.0.0",
        "drupal/entity_reference_revisions": "^1.7",
        "drupal/entity_usage": "^2.0.0-alpha7",
        "drupal/eva": "^2.0",
        "drupal/externalauth": "^1.2.0",
        "drupal/facets": "^1.3",
        "drupal/facets_block": "^1.2",
        "drupal/field_group": "3.1",
        "drupal/file_access_fix": "1.x-dev",
        "drupal/file_download_link": "^1.0",
        "drupal/file_replace": "^1.2",
        "drupal/geolocation": "^3.0",
        "drupal/google_analytics": "^2.4.0",
        "drupal/google_tag": "^1.4",
        "drupal/hook_post_action": "^1.0",
        "drupal/html_title": "1.x-dev",
        "drupal/jwt": "^1",
        "drupal/languagefield": "^1.5",
        "drupal/ldap": "^4",
        "drupal/linked_field": "^1.3",
        "drupal/masquerade": "^2.0@beta",
        "drupal/memcache": "^2.1",
        "drupal/migrate_tools": "^5",
        "drupal/name": "^1.0@RC",
        "drupal/node_access_rebuild_progressive": "^2",
        "drupal/paragraphs": "^1.10",
        "drupal/pathauto": "^1.8",
        "drupal/pdf": "1.x-dev",
        "drupal/readmehelp": "^1.0@beta",
        "drupal/redirect": "^1.7",
        "drupal/rest_oai_pmh": "1.x-dev",
        "drupal/restui": "^1.16",
        "drupal/search_api": "^1.15",
        "drupal/search_api_field_map": "^3.1",
        "drupal/search_api_solr": "^4.2",
        "drupal/select2": "^1.12",
        "drupal/simple_timeline": "^1.1.0",
        "drupal/slick": "^2.2",
        "drupal/switch_page_theme": "^1.0.0",
        "drupal/tvi": "^1.0@RC",
        "drupal/twig_tweak": "^2",
        "drupal/views_bulk_edit": "^2.6",
        "drupal/views_bulk_operations": "^4",
        "drupal/views_conditional": "*",
        "drupal/views_contextual_filters_or": "^1.2",
        "drupal/views_data_export": "^1.1",
        "drupal/views_entity_form_field": "^1.0@beta",
        "drupal/views_field_formatter": "^1.10",
        "drupal/views_infinite_scroll": "^1.7",
        "drupal/views_timelinejs": "^3.1.0",
        "drush/drush": "^10",
        "grasmash/drupal-security-warning": "^1.0.0",
        "islandora/chullo": "^1",
        "islandora/controlled_access_terms": "^2",
        "islandora/crayfish-commons": "^2",
        "islandora/islandora": "^2",
        "islandora/islandora_defaults": "^2",
        "islandora/jsonld": "^2",
        "islandora/openseadragon": "^2",
        "maennchen/zipstream-php": "^1.2.0",
        "mjordan/islandora_workbench_integration": "dev-main",
        "natkeeran/views_nested_details": "^0.0.0",
        "npm-asset/nouislider": "^14",
        "solarium/solarium": "^6.1.3",
        "unlv/islandora_local": "dev-master",
        "unlv_wads/newspapers": "dev-main",
        "unlv_wads/special_theme": "^1",
        "wikimedia/composer-merge-plugin": "^2",
        "woocommerce/flexslider": "~2.0"
    },
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "optimize-autoloader": true,
        "bitbucket-oauth": {

        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
              "Fix large taxonomy admin scaling issue": "https://www.drupal.org/files/issues/2019-07-08/763380-61.patch",
              "Negate form value for condition plugins should be cast to boolean in validation": "https://www.drupal.org/files/issues/2021-01-06/3114467-16.patch",
              "Issue 3180227 EntityReference Warning": "https://www.drupal.org/files/issues/2020-11-02/3180227-2-8.9.x.patch",
	      "Issue 2949017 Allow deleting files": "https://www.drupal.org/files/issues/2022-03-24/allow-user-with-delete-any-files-to-delete-in-bulk.patch"
            },
            "drupal/facets": {
              "noUiSlider": "https://www.drupal.org/files/issues/2021-06-16/issue-3153622-nouislider_4.patch"
            },
            "drupal/ldap": {
              "Issue 3206174 Fatal Error": "https://git.drupalcode.org/project/ldap/-/merge_requests/16.diff"
            },
	    "islandora/controlled_access_terms": {"EDTF Year Formatting": "https://patch-diff.githubusercontent.com/raw/Islandora/controlled_access_terms/pull/76.diff"},
	    "islandora/islandora": {
		    "Improved breadcrumbs": "https://patch-diff.githubusercontent.com/raw/Islandora/islandora/pull/873.patch",
	            "Fix Broken Media WSOD": "https://patch-diff.githubusercontent.com/raw/Islandora/islandora/pull/874.patch"
	    }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ],
            "core": [
                "type:drupal-core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "merge-plugin": {
            "include": [
                "modules/custom/*/composer.json"
            ],
            "replace": false,
            "ignore-duplicates": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
